
<Window x:Class="StockApp.StockListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Stock List" Height="350" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <Label Content="Available Stocks" FontSize="16"/>
            <ListBox x:Name="StockListBox" SelectionChanged="StockListBox_SelectionChanged" Height="200"/>
            <Button Content="Back to Dashboard" Click="BackButton_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
